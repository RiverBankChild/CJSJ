create table dmb (dm varchar(8) primary key ,name varchar(8) default'未知名称');
create table IF NOT EXISTS tb+dm (date varchar(16) primary key default '未知',open Float(5,2) default  0.00,close Float(5,2) default  0.00,high Float(5,2) default  0.00,low Float(5,2) default  0.00,volume Float(14,2) default  0.00);
